  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "config": "^3.0.1",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.4.0",
    "mongodb": "^3.1.13",
    "path": "^0.12.7"
  }
}
